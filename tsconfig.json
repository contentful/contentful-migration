{
  "compilerOptions": {
    "outDir": "./built",
    "allowJs": true,
    "target": "es6",
    "module": "commonjs",
    "lib": [
      "es2015",
      "es2016",
      "es2017",
      "esnext"
    ],
    "inlineSourceMap": true,
    "typeRoots": [
      "./typings",
      "./node_modules/@types"
    ],
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": [
    "./src/**/*"
  ]
}