{
  "compilerOptions": {
    "outDir": "./built",
    "allowJs": true,
    "target": "es2017",
    "esModuleInterop":true,
    "module": "commonjs",
    "lib": ["esnext"],
    "sourceMap": true,
    "typeRoots": [
      "./typings",
      "./node_modules/@types"
    ],
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": [
    "./src/**/*"
  ]
}
